<MudNavMenu>
    <MudNavLink Href="" Match="NavLinkMatch.All" Icon="@Icons.Material.Filled.Home">Home</MudNavLink>
    @if (IsVersionB)
    {
        <MudNavLink Href="makequickreservation" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.Add">makequickreservation</MudNavLink>
    }
    else
    {
        <MudNavLink Href="makecompletereservation" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.Add">makecompletereservation</MudNavLink>

    }
    </MudNavMenu>

    @code
    {
    protected bool IsVersionB { get; set; }

    [Inject] private IConfiguration _configuration { get; set; }

    protected override void OnInitialized()
    {
        IsVersionB = Convert.ToBoolean(_configuration["VersionB"]);
    }
}